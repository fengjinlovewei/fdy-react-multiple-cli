{
  "name": "fdy-react-multiple-cli",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm run tools && cross-env NODE_ENV=development SERVER_HTTP=true webpack-dev-server -c script/webpack.dev.js",
    "build": "npm run tools && cross-env NODE_ENV=production webpack -c script/webpack.prod.js",
    "build:analy": "npm run tools && cross-env NODE_ENV=production webpack -c script/webpack.analy.js",
    "build:open": "npm run tools && cross-env NODE_ENV=production SERVER_HTTP=true webpack -c script/webpack.prod.js && http-server dist -p 3002 -d false --no-dotfiles -o index",
    "build:stats": "npm run tools && cross-env NODE_ENV=production webpack -c script/webpack.prod.js --json > stats.json",
    "dll:core": "cross-env NODE_ENV=production webpack -c script/webpack.vendor-core.config.js",
    "lint": "eslint src --fix",
    "tools": "node script/tools.mjs",
    "debugger": "cross-env NODE_ENV=production node --inspect-brk  ./node_modules/webpack/bin/webpack.js -c script/webpack.prod.js",
    "prepare": "husky install && git config --local core.editor cat",
    "test": "cross-env NODE_ENV=test && jest --watch"
  },
  "author": "fengjin",
